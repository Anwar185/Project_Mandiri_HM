    <record id="report_balita_xlsx" model="ir.actions.report">
        <field name="name">Print Balita Excel</field>
        <field name="model">posyandu.balita</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">posyandu_report_balita_xlsx</field>
